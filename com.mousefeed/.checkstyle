<?xml version="1.0" encoding="UTF-8"?>

<fileset-config file-format-version="1.2.0" simple-config="true" sync-formatter="false">
  <local-check-config name="MouseFeed" location="checkstyle.xml" type="project" description="MouseFeed project coding conventions">
    <additional-data name="protect-config-file" value="true"/>
  </local-check-config>
  <fileset name="all" enabled="true" check-config-name="MouseFeed" local="true">
    <file-match-pattern match-pattern="." include-pattern="true"/>
  </fileset>
  <filter name="FilesFromPackage" enabled="true">
    <filter-data value="test"/>
  </filter>
  <filter name="NonSrcDirs" enabled="true"/>
</fileset-config>
